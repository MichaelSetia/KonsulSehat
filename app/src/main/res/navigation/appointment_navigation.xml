<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/appointment_navigation"
    app:startDestination="@id/appointmentUpcoming">

    <fragment
        android:id="@+id/appointmentFinish"
        android:name="com.example.konsulsehat.dokter.AppointmentFinish"
        android:label="fragment_appointment_finish"
        tools:layout="@layout/fragment_appointment_finish" />
    <fragment
        android:id="@+id/appointmentUpcoming"
        android:name="com.example.konsulsehat.dokter.AppointmentUpcoming"
        android:label="fragment_appointment_upcoming"
        tools:layout="@layout/fragment_appointment_upcoming" />
    <action
        android:id="@+id/action_global_appointmentFinish"
        app:destination="@id/appointmentFinish" />
    <action
        android:id="@+id/action_global_appointmentUpcoming"
        app:destination="@id/appointmentUpcoming" />
</navigation>