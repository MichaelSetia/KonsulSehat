<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_navigation"
    app:startDestination="@id/masterAppointmentFragment">

    <fragment
        android:id="@+id/masterAppointmentFragment"
        android:name="com.example.konsulsehat.Admin.MasterAppointmentFragment"
        android:label="fragment_master_appointment"
        tools:layout="@layout/fragment_master_appointment" />
    <fragment
        android:id="@+id/masterTransaksiFragment"
        android:name="com.example.konsulsehat.Admin.MasterTransaksiFragment"
        android:label="fragment_master_transaksi"
        tools:layout="@layout/fragment_master_transaksi" />
    <fragment
        android:id="@+id/masterUserFragment"
        android:name="com.example.konsulsehat.Admin.MasterUserFragment"
        android:label="fragment_master_user"
        tools:layout="@layout/fragment_master_user" />
    <action
        android:id="@+id/action_global_masterAppointmentFragment"
        app:destination="@id/masterAppointmentFragment" />
    <action
        android:id="@+id/action_global_masterTransaksiFragment"
        app:destination="@id/masterTransaksiFragment" />
    <action
        android:id="@+id/action_global_masterUserFragment"
        app:destination="@id/masterUserFragment" />
</navigation>